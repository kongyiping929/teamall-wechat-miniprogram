page {
    background-color: #F4F4F4;
}

/* 预约tip */
.titleTip {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
    height: 95rpx;
}

.titleTip text:first-child {
    position: relative;
    font-size: 36rpx;
    font-weight: 500;
    color: rgba(66, 76, 80, 1);
}

.titleTip text:first-child::after {
    content: '';
    position: absolute;
    top: 5rpx;
    left: -30rpx;
    width: 9rpx;
    height: 39rpx;
    background-color: #799262;
    border-top-right-radius: 8rpx;
    border-bottom-right-radius: 8rpx;
}

.titleTip.square text:first-child::after {
    background-color: #F9C831;
}

.titleTip.clock text:first-child::after {
    background-color: #83CDE3;
}

.titleTip navigator {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
}

/* 预约订单 */
.order {
    width: 690rpx;
    /* height: 430rpx; */
    background: rgba(255, 255, 255, 1);
    border-radius: 20rpx;
    margin: 0 auto 26rpx;
}

.orderTime {
    display: flex;
    align-items: center;
    padding: 24rpx 22rpx 20rpx;
    height: 50rpx;
}

.orderTime text {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
}

.orderTime image {
    width: 24rpx;
    height: 24rpx;
    vertical-align: middle;
    margin-right: 18rpx;
}

.orderMes {
    display: flex;
    height: 160rpx;
    padding: 0 24rpx;
    margin-bottom: 36rpx;
}

.orderMesPhoto {
    width: 160rpx;
    height: 160rpx;
    margin-right: 28rpx;
}

.orderMesTip {
    width: 438rpx;
}

.orderMesTipTitle {
    height: 45rpx;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 45rpx;
    color: rgba(66, 76, 80, 1);
    margin-bottom: 26rpx;
}

.orderMesTipAttr {
    /* display: flex;
    align-items: center; */
    display: inline-block;
    height: 46rpx;
    line-height: 36rpx;
    background: rgba(243, 248, 241, 1);
    border-radius: 8rpx;
    padding: 0 14rpx;
}

.orderMesTipAttr text {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
    margin-right: 6rpx;
}

.orderMesTipAttr image {
    width: 24rpx;
    height: 24rpx;
}

.orderAddress {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 24rpx;
    height: 36rpx;
    margin-bottom: 32rpx;
}

.orderAddress image {
    width: 24rpx;
    height: 29rpx;
}

.orderAddress text:nth-child(2) {
    width: 261rpx;
    font-size: 22rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
}

.orderAddress text:nth-child(3),
.orderAddress text:nth-child(5) {
    width: 1rpx;
    height: 33rpx;
    background-color: rgba(215, 224, 224, 1);
}

.orderAddress view:nth-child(4) {
    width: 160rpx;
    height: 48rpx;
    display: flex;
    align-items: center;
}

.orderAddress view:nth-child(4) text:first-child {
    font-size: 22rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
    margin-right: 8rpx;
}

.orderAddress view:nth-child(4) text:last-child {
    font-size: 22rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
}

.orderAddress text:nth-child(6) {
    width: 120rpx;
    font-size: 22rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
}

.orderPay {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 92rpx;
    padding: 0 24rpx;
    border-top: 1rpx solid rgba(215, 224, 224, 1);
}

.orderPay view:first-child {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
}

.orderPay view:last-child {
    width: 150rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    background: rgba(247, 55, 2, 1);
    border-radius: 30rpx;
    font-size: 26rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

/* 微广场产品类型 */
.productIcon {
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    width: 690rpx;
    height: 475rpx;
    padding: 48rpx;
    background-color: #fff;
    border-radius: 20rpx;
    margin: 0 auto 30rpx;
}

.productIconItem {
    width: 120rpx;
    height: 177rpx;
    margin-right: 10rpx;
    margin-bottom: 20rpx;
}

.productIconItem image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 60rpx;
    vertical-align: middle;
    margin-bottom: 24rpx;
}

.productIconItem view {
    text-align: center;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
}

/* 分割线 */
.halving {
    height: 1rpx;
    background-color: #D7E0E0;
    margin-bottom: 16rpx;
}

/* 打卡信息 */
.clockWapper {
    box-sizing: border-box;
    padding: 40rpx 40rpx 42rpx;
    width: 690rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 20rpx;
    margin: 0 auto 20rpx;
}

.clockWapper.length {
    padding-bottom: 33rpx;
    margin-bottom: 0;
}

.clockTitleBar {
    display: flex;
    justify-content: space-between;
    height: 78rpx;
    margin-bottom: 24rpx;
}

.clockTitleBarLeft,
.clockTitleBarRight {
    display: flex;
    height: 78rpx;
    align-items: center;
}

.clockTitleBarLeft image {
    width: 78rpx;
    height: 78rpx;
    margin-right: 41rpx;
}

.clockTitleBarLeft text {
    font-size: 34rpx;
    font-weight: 600;
    color: rgba(66, 76, 80, 1);
}

.clockTitleBarRight image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}

.clockTitleBarRight text {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
}

.clockContent {
    position: relative;
    height: 180rpx;
    line-height: 36rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
    margin-bottom: 36rpx;
}

.clockContent.none {
    height: auto;
}

.clockContent text {
    position: absolute;
    bottom: 3rpx;
    right: 0;
    width: 52rpx;
    height: 37rpx;
    font-size: 26rpx;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 37rpx;
    color: rgba(121, 146, 98, 1);
    z-index: 5;
    background-color: #fff;
}

.clockImg {
    margin-bottom: 4rpx;
}

.clockImg.length {
    margin-bottom: 36rpx;
}

.clockImg image {
    width: 180rpx;
    height: 180rpx;
    margin-right: 34rpx;
}

.clockImg image:nth-child(3n) {
    margin-right: 0;
}

.TorderMes {
    box-sizing: content-box;
    display: flex;
    height: 160rpx;
    padding-top: 32rpx;
    border-top: 1rpx dashed rgba(215, 224, 224, 1);
}

.TorderMesPhoto {
    width: 160rpx;
    height: 160rpx;
    margin-right: 28rpx;
}

.TorderMesTip {
    width: 416rpx;
}

.TorderMesTipTitle {
    height: 45rpx;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 45rpx;
    color: rgba(66, 76, 80, 1);
    margin-bottom: 10rpx;
}

.TorderMesPrice {
    display: flex;
    align-items: center;
    height: 45rpx;
    margin-bottom: 21rpx;
}

.TorderMesPrice text:first-child {
    font-size: 32rpx;
    font-weight: 600;
    color: rgba(247, 55, 2, 1);
    margin-right: 12rpx;
}

.TorderMesPrice text:last-child {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
}

.TorderMesTipAttr {
    /* display: flex;
    align-items: center; */
    display: inline-block;
    height: 46rpx;
    background: rgba(243, 248, 241, 1);
    border-radius: 8rpx;
    padding: 0 14rpx;
}

.TorderMesTipAttr text {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
    margin-right: 6rpx;
}

.TorderMesTipAttr image {
    width: 24rpx;
    height: 24rpx;
}

/* 底部 */
.footer {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 97rpx;
    font-size: 24rpx;
    font-weight: 400;
    color:rgba(172,187,188,1);
}