/* 预约订单 */
/* 公告 */
.notice{
  display: flex;
  align-items: flex-start;
  font-size: 24rpx;
  background-color: #9DAF84;
  color: #fff;
  padding: 16rpx 30rpx;
}
.radius{
  color: #9DAF84;
  padding:0 16rpx;
  border-radius: 50%;
  background: rgba(255, 255, 255, 1);
  margin-right: 5rpx;
}
/* 预约列表 */
.subscribe,
.order {
    box-sizing: border-box;
    width: 690rpx;
    margin: 20rpx auto;
    background: rgba(255, 255, 255, 1);
    border-radius: 20rpx;
    padding: 0 32rpx;
}

.subscribeTimeMes {
    height: 80rpx;
    display: flex;
    align-items: center;
}

.subscribeTimeMes text:nth-child(1) {
    width: 496rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
}

.subscribeTimeMes text:last-child {
    width: 308rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
    text-align: right;
}

.subscribeTitle {
    display: flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 0 0 20rpx;
    height: 64rpx;
    font-size: 32rpx;
    font-weight: 400;
    color: rgba(66, 76, 80, 1);
}

.subscribeAttr {
    display: inline-block;
    align-items: center;
    padding: 4rpx 14rpx 8rpx;
    height: 34rpx;
    line-height: 34rpx;
    background: rgba(243, 248, 241, 1);
    border-radius: 8rpx;
    margin-bottom: 20rpx;
}

.subscribeAttr text {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
    margin-right: 8rpx;
}

.subscribeAttr image {
    width: 24rpx;
    height: 24rpx;
    vertical-align: middle;
}

.subscribePrice {
    padding-top: 14rpx;
    padding-bottom: 28rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    font-weight: 600;
    color: rgba(247, 55, 2, 1);
}
.subscribePrice2 {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-bottom: 14rpx;
    height: 40rpx;
    font-size: 28rpx;
    font-weight: 600;
    color: rgba(247, 55, 2, 1);
}

.subscribePrice2 text:last-child {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
}
.subscribeAddress {
    height: 34rpx;
    display: flex;
    align-items: center;
    padding-bottom: 34rpx;
}

.subscribeAddress image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 6rpx;
}

.subscribeAddress text:nth-child(2) {
    max-width: 280rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
    margin-right: 6rpx;
}

.subscribeAddress text:nth-child(3) {
    width: 1rpx;
    height: 34rpx;
    background-color: rgba(215, 224, 224, 1);
    margin-right: 10rpx;
}

.subscribeAddress text:nth-child(4) {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
    margin-right: 10rpx;
}

.subscribeAddress text:nth-child(5) {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(121, 146, 98, 1);
}

.subscribeNumber {
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 690rpx;
    height: 92rpx;
    margin-left: -32rpx;
    padding: 0 32rpx;
    border-top: 1rpx solid rgba(215, 224, 224, 1);
}

.subscribeNumber text:first-child {
    font-size: 26rpx;
    font-weight: 400;
    color: #424C50;
}

.subscribeNumber text:last-child {
    font-size: 26rpx;
    font-weight: bold;
    color: rgba(247, 55, 2, 1);
    margin-left: 10rpx
}
.subscribeNumber .confirm {
    width: 160rpx;
    height: 60rpx;
    border:1px solid rgba(247, 55, 2, 1);
    border-radius: 30rpx;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 60rpx;
    color: rgba(247, 55, 2, 1);
    text-align: center;
}
.subscribeNumber view:first-child{
  flex: 1;
}
.subscribeNumber view:last-child {
    width: 160rpx;
    height: 60rpx;
    background: rgba(247, 55, 2, 1);
    border-radius: 30rpx;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 60rpx;
    color: rgba(255, 255, 255, 1);
    text-align: center;
    margin-left: 20rpx
}
.subscribeOrderNum {
    height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.subscribeOrderNum text:nth-child(1) {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
}

.subscribeOrderNum text:last-child {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(172, 187, 188, 1);
}
.subscribeProson {
    display: flex;
    align-items: center;
    height: 34rpx;
    margin-bottom: 12rpx;
}

.subscribeProson image {
    width: 32rpx;
    height: 32rpx;
    margin-right: 8rpx;
}

.subscribeProson text:nth-child(2) {
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(66, 76, 80, 1);
    margin-right: 8rpx;
}

.subscribeProson text:nth-child(3) {
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(85, 98, 102, 1);
}
.subscribeConsignee {
    padding-bottom: 24rpx;
}

.subscribeConsignee text:first-child {
    padding-left: 40rpx;
    font-size: 24rpx;
    font-weight: 400;
    line-height: 34rpx;
    color: rgba(140, 153, 155, 1);
    margin-right: 10rpx;
}

.subscribeConsignee text:last-child {
    font-size: 24rpx;
    font-weight: 400;
    line-height: 34rpx;
    color: rgba(85, 98, 102, 1);
}