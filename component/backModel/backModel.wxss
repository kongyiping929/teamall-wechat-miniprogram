.place-holder{
  font-size: 28rpx;
  color: #ACBBBC
}
.mask_layer {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999;
  left:0;top:0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}
.modal_box {
  width: 76%;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 1001;
  background: #fafafa;
  margin: -150px 12% 0 12%;
  border-radius: 10px;
}
 
.title {
  font-size: 30rpx;
  font-weight: 600;
  color: #424C50;
  padding: 40rpx 30rpx;
  text-align: center;
  background-color: gazure;
}
 
.content {
  overflow-y: scroll; /*超出父盒子高度可滚动*/
  padding: 0 60rpx
}
 .data{
   font-size: 30rpx;
 }
 .data text{
   color: #44b549
 }
 .input_show1{
   margin: 20rpx 0;
   height: 76rpx !important;
   line-height: 76rpx;
   font-size: 34rpx;
   background-color: #eaeaea;
   border-radius: 14rpx;
   padding-left: 22rpx;
 }
.btn1 {
  width: 100%;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: white;
}
 
.cancel {
  width: 100%;
  padding: 10px;
  text-align: center;
  color: black;
}
 
.Sure {
  width: 100%;
  padding: 10px;
  color: #44b549;
  background-color: white;
  border-left: 1px solid #f0f0f0;
  text-align: center;
}
 
.modalMsg {
  text-align: center;
  margin-top: 45rpx;
  display: block;
}
.box{
  display: flex;
  font-size: 30rpx;
  justify-content: space-between;
  align-items: center;
  padding: 0 10rpx;
}

.type{
  font-size: 24rpx;
  color: #fff;  
  padding: 12rpx 18rpx;
  background-color: #9DAF84;
  border-radius: 50%;
  margin-right: 12rpx
}
.selected{
  font-size: 30rpx;
  color: #424C50;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 20rpx; 
  line-height: 80rpx;
  border: 1px #D7E0E0 solid;
  box-shadow: 0 0 8rpx #D7E0E0;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
}
.selected image{
  width: 24rpx;
  height: 24rpx;
}
.active{
  border-color: #799262;
}
.jia{
  background-color: #C69C6D
}
.hui{
  background-color: #ED5735
}