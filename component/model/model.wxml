<view wx:if='{{showModal}}'>
  <view class='mask_layer' bindtap='modal_click_Hidden' />
  <view class='modal_box'>
    <view class="title">温馨提示</view>
    <view class='content'>
      <view class="box" wx:if='{{showSwitch}}'>
        <text>预约开关：</text>
        <switch class="wx-switch-input" checked="true" bindchange="switch1Change"/>
      </view>
      <view wx:if='{{showStock}}'>
        <text class="data">当前库存：<text>{{data.stockNum || 0}}</text></text>
        <input class='input_show1' bindinput='inputChange' value='{{modalMsg}}'  placeholder='输入库存'></input>
      </view>
    </view>
    <view class='btn1'>
      <view bindtap='modal_click_Hidden' class='cancel'>取消</view>
      <view bindtap='Sure' class='Sure'>确定</view>
    </view>
  </view>
</view>
